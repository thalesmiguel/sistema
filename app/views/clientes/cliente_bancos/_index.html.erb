<div class="p-t-0 paginate_left">
  <table id="cliente_bancos-table" class="highlight bordered" data-source="<%= cliente_cliente_bancos_path(@cliente_cliente_banco, format: :json) %>" data-cliente-cliente_banco="<%= @cliente_cliente_banco %>">
    <thead>
      <tr>
        <th width="10%">Código</th>
        <th width="10%">Descrição</th>
        <th width="10%">Agência</th>
        <th width="10%">CC</th>
        <th width="10%">Cidade</th>
        <th width="5%">UF</th>
        <th width="10%">Correntista</th>
        <th width="10%">CPF/CNPJ</th>
        <th width="10%">Observação</th>
        <th width="15%">Dt. Cadastro</th>
        <th width="5%">Primário</th>
        <th width="5%">Ativo</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>

  <div class="row m-b-0 m-t-30">
    <div class="valign-wrapper">
      <div class="col s12 valign right-align">
        <%= link_to_new new_cliente_cliente_banco_path, true, "", "Novo Banco" if can? :manage, :cliente_banco %>
      </div>
    </div>
  </div>
</div>
