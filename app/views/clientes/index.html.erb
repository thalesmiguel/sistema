<!-- Modals -->
<div id="endereco_modal" class="modal bottom-sheet modal-grande"></div>
<div id="telefone_modal" class="modal bottom-sheet modal-grande"></div>
<div id="email_modal" class="modal bottom-sheet modal-grande"></div>
<div id="fazenda_modal" class="modal bottom-sheet modal-grande"></div>
<div id="cliente_banco_modal" class="modal bottom-sheet modal-grande"></div>
<div id="referencia_modal" class="modal bottom-sheet modal-grande"></div>
