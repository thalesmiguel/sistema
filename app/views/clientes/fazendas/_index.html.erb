<div class="p-t-0 paginate_left">
  <table id="fazendas-table" class="highlight bordered" data-source="<%= cliente_fazendas_path(@cliente_fazenda, format: :json) %>" data-cliente-fazenda="<%= @cliente_fazenda %>">
    <thead>
      <tr>
        <th width="20%">Nome <%= filtro_dt %> </th>
        <th width="10%">Cidade <%= filtro_dt %> </th>
        <th width="10%">UF <%= filtro_dt %> </th>
        <th width="10%">Inscrição <%= filtro_dt %> </th>
        <th width="10%">CNPJ <%= filtro_dt %> </th>
        <th width="10%">CNPJ Produtor <%= filtro_dt %> </th>
        <th width="10%">Ativa <%= filtro_dt %> </th>
        <th width="10%">Vendas <%= filtro_dt %> </th>
        <th width="10%">Compras <%= filtro_dt %> </th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>

  <div class="row m-b-0 m-t-30">
    <div class="valign-wrapper">
      <div class="col s12 valign right-align">
        <%= link_to_new new_cliente_fazenda_path, true, "", "Nova Fazenda" if can? :manage, :fazenda %>
      </div>
    </div>
  </div>
</div>
